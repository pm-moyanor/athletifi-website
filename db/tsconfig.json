{
  "compilerOptions": {
    "jsx": "react",
    "types": ["node"],
    "target": "ES2022",
    "lib": [ "ES2022"],
    "downlevelIteration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "rootDir": "..",
    "outDir": "build"
  }
}
